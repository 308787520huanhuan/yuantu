			<form id="validate" action="./myAppAdd" method="post"  class="form-horizontal form-bordered">

				<!-- Basic inputs -->
				<div class="panel panel-default">
					<div class="panel-heading">
						<h6 class="panel-title">
							<i class="icon-bubble4"></i>编辑应用详情
						</h6>
					</div>
					<div class="panel-body">
						
						<div class="form-group">
							<label class="col-sm-2 control-label text-right">游戏英文名字: <span class="mandatory">*</span></label>
							<div class="col-sm-4">
								<input type="text" class="form-control" name="name" id="name" value="<%= gameData['name'] %>"/>
								<input id="gameid" name="gameid" type="hidden" value="<%= gameData['ID'] %>"/>
							</div>
						</div>
						
						
						<div class="form-group">
							<label class="col-sm-2 control-label text-right">游戏中文名字: <span class="mandatory">*</span></label>
							<div class="col-sm-8">
								<input type="text" class="form-control" name="appName" id="appName" value="<%= gameData['appName'] %>"/>
							</div>
						</div>
						
						<div class="form-group">
							<label class="col-sm-2 control-label text-right">游戏入口地址: <span class="mandatory">*</span>
							</label>
							<div class="col-sm-8"> 
                                <input type="text" class="form-control" name="loginUrl" id="loginUrl" value="<%= gameData['loginUrl'] %>" />
							</div>
						</div>
						
						<div class="form-group">
							<label class="col-sm-2 control-label text-right">secretKey: <span class="mandatory">*</span></label>
							<div class="col-sm-4">
                                <input type="text" class="form-control" name="secretKey" id="secretKey" value="<%= gameData['secretKey'] %>" />
							</div>
						</div>
						
						<div class="form-group">
							<label class="col-sm-2 control-label text-right">支付回调地址: <span class="mandatory">*</span></label>
							<div class="col-sm-8">
                                <input type="text" class="form-control" name="paybackUrl" id="paybackUrl" value="<%= gameData['paybackUrl'] %>" />
							</div>
						</div>
						
						<div class="form-actions text-left"> 
							<input type="reset" value="重 置" class="btn btn-danger">
							<input type="submit" value="提 交" class="btn btn-primary"> 
						</div>
					</div>
				</div>
				
			</form>
		
			

