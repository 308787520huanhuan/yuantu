			<form id="validate" action="../myProductPaging/myProductPaging" method="post" >
				<!-- Basic inputs -->
				<div class="panel panel-default">
					<div class="panel-heading">
						<h6 class="panel-title">
							<i class="icon-bubble4"></i>条件筛选
						</h6>
					</div>
					<div class="panel-body"> 
						<div class="form-group">
							<div class="row">
								<div class="col-md-3">
									<label>平台名称:</label>
                                    <select name="platform" class="form-control">
                                        <% option.forEach(function(data){%>
                                        <option value="<%= data['platform']%>"><%= data["platform1"] %></option>
                                        <%})%>
                                    </select>
								</div>

                                <div class="col-md-3">
									<label>游戏名称:</label>
                                   <select  name="gameID" class="form-control">
                                      <% game.forEach(function(data){ %>
